  "ReverseProxy": {
    "Routes": {
      "order-route": {
        "ClusterId": "orders-cluster",
        "RateLimiterPolicy": "customPolicy",
        "Match": {
          "Path": "/orders/{*any}",
          "Methods": [ "POST" ]
        },
        "Transforms": [ { "PathPattern": "/api/PurchaseOrder" } ]
      },
      "invoice-route": {
        "ClusterId": "invoices-cluster",
        "RateLimiterPolicy": "customPolicy",
        "Match": {
          "Path": "/invoices/{*any}",
          "Methods": [ "POST" ]
        },
        "Transforms": [ { "PathPattern": "/api/Invoice" } ]
      },
      "asn-route": {
        "ClusterId": "asn-cluster",
        "RateLimiterPolicy": "customPolicy",
        "Match": {
          "Path": "/asn/{*any}",
          "Methods": [ "POST" ]
        },
        "Transforms": [ { "PathPattern": "/api/ShipmentNotice" } ]
      }
    },
    "Clusters": {
